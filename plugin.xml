<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         id="voladroid"
         name="Voladroid"
         point="org.eclipse.core.runtime.applications">
      <application cardinality="*" thread="any">
         <run 
               class="VoladroidApplication">
         </run>
      </application>
   </extension>
   <extension
         id="console"
         name="VoladroidConsole"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="*"
            thread="any">
         <run
               class="VoladroidConsole">
         </run>
      </application>
   </extension>

</plugin>
